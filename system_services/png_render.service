[Unit]
Description=PNG render display image service
After=network.target

[Service]
WorkingDirectory=/home/pi/Desktop/App/images
ExecStart=/home/pi/PiCrown4.0/raspidmx/png_disp_service/png_disp_service /home/pi/PiCrown4.0/screen.png

Restart=always
RestartSec=1 

[Install]
WantedBy=multi-user.target
